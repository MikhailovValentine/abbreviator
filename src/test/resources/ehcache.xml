<config xmlns:jsr107='http://www.ehcache.org/v3/jsr107'
        xmlns='http://www.ehcache.org/v3'>

    <service>
        <jsr107:defaults>
            <jsr107:cache name="urls" template="urls-cache"/>
        </jsr107:defaults>
    </service>

    <cache-template name="urls-cache">
        <resources>
            <heap>2000</heap>
            <offheap unit="MB">100</offheap>
        </resources>
    </cache-template>
</config>